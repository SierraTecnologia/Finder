{
    "name": "sierratecnologia/finder",
    "description": "Finder de Arquivos",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "symfony/finder": "^4.4.0",
        "cloudflare/sdk": "*",
        "deployer/deployer": "^6.4",
        "devio/pipedrive": "*",
        "fideloper/proxy": "^4.0",
        "sebastian/diff": "*",
        "maatwebsite/excel": "^3.1",
        "sabre/xml": "*",
        "league/csv": "^7.1",
        "league/flysystem": "*",
        "league/flysystem-cached-adapter": "*",
        "league/flysystem-sftp": "*",
        "league/flysystem-ziparchive": "*",
        "srmklive/flysystem-dropbox-v2": "*",
        "google/photos-library": "^1.4",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "spatie/laravel-medialibrary": "^7.9.0",
        "league/climate": ">=3.4",
        "sierratecnologia/tracking": "dev-master",
        "sierratecnologia/casa": "dev-master",
        "pulkitjalan/google-apiclient": "^4.0",
        "cache/filesystem-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Finder\\": "src/"
        }
    },
    "bin": ["bin/observador"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Finder\\FinderProvider"
            ]
        }
    }
}
