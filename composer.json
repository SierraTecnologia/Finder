{
    "name": "sierratecnologia/finder",
    "description": "Finder de Arquivos",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "finder"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/finder/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/finder/issues",
        "source": "https://github.com/sierratecnologia/finder",
        "docs": "https://github.com/sierratecnologia/finder/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^7.0 || ^8.0",
        "symfony/finder": "^4.0|^5.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "nicmart/string-template": "v0.1.0",
        "fideloper/proxy": ">=4.0",
        "ricardosierra/robo-docker-compose": "^0.0.10",
        "thujohn/twitter": ">=2.2",
        "markrogoyski/math-php": ">=1.0",
        "sierratecnologia/operador": "dev-master",
        "php-ai/php-ml": ">=0.8",
        "sensiolabs/security-checker": "*",
        "lesstif/php-jira-rest-client": "*",
        "m4tthumphrey/php-gitlab-api": "*",
        "texthtml/docker-links": "*",
        "maatwebsite/excel": ">=3.1",
        "sabre/xml": "^2.0",
        "league/csv": "^9.0|^8.0",
        "league/flysystem": "^1.1|^2.0",
        "league/flysystem-cached-adapter": "^1.0",
        "google/photos-library": ">=1.4",
        "league/pipeline": "^1.0",
        "league/climate": ">=3.4",
        "sierratecnologia/crypto": "dev-master",
        "privatejustice/tramite": "dev-master",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/dom-crawler": "*",
        "cache/filesystem-adapter": ">=1.0",
        "sierratecnologia/stalker": "dev-master",
        "sierratecnologia/casa": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "sierratecnologia/fabrica": "dev-master",
        "phpoffice/phppresentation": "*",
        "phpoffice/phpspreadsheet": "*",
        "phpoffice/phpword": "*",
        "lorisleiva/laravel-actions": "^1.1",
        "halaxa/json-machine": "^0.5.0",
        "paquettg/php-html-parser": "^3.1"
    },
    "suggest": {
        "spatie/crawler": "*",
        "symfony/dom-crawler": "*",
        "league/flysystem-sftp": "^1.0|^2.0",
        "srmklive/flysystem-dropbox-v2": "^1.0",
        "raiym/instagram-php-scraper": ">=0.9.10",
        "jaeger/querylist": ">=4.1"
    },
    "autoload": {
        "psr-4": {
            "Finder\\": "src/"
        }
    },
    "bin": ["bin/observador"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [{"type": "path","url":"../**","options":{"symlink": true}}],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Finder\\FinderProvider"
            ]
        }
    },
    "require-dev": {
        "phpro/grumphp": "^1.3"
    }
}
